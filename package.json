{
  "name": "hackaru-desktop",
  "version": "1.1.1",
  "author": "ktmouk <ktmouk@gmail.com>",
  "description": "Hackaru for desktop",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktmouk/hackaru-desktop"
  },
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue src __tests__",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "jest",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run license-third-party && electron-builder install-app-deps",
    "license-third-party": "npm-license-crawler --onlyDirectDependencies --omitVersion --json src/renderer/assets/licenses.json"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run lint -- --fix",
      "git add"
    ]
  },
  "build": {
    "productName": "Hackaru",
    "appId": "app.hackaru",
    "afterSign": ".electron-vue/notarize",
    "asarUnpack": [
      "./node_modules/node-notifier/vendor/**"
    ],
    "extraResources": [
      "./extra-resources"
    ],
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "appImage": {
      "artifactName": "${name}-${version}.${ext}"
    },
    "nsis": {
      "artifactName": "${name}-${version}.${ext}"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "publish": {
      "provider": "github",
      "releaseType": "draft",
      "publishAutoUpdate": true
    }
  },
  "dependencies": {
    "@sentry/browser": "^5.6.3",
    "@sentry/electron": "^0.17.4",
    "@sentry/integrations": "^5.7.1",
    "axios": "^0.19.2",
    "camelcase-keys": "^6.2.1",
    "date-fns": "^2.9.0",
    "electron-log": "^3.0.7",
    "electron-promise-ipc": "^1.3.0",
    "electron-store": "^5.0.0",
    "electron-updater": "^4.2.0",
    "feather-icons": "^4.10.0",
    "hh-mm-ss": "^1.2.0",
    "keytar": "^4.13.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "lodash.union": "^4.6.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "menubar": "^6.0.7",
    "node-machine-id": "^1.1.12",
    "node-notifier": "^6.0.0",
    "normalize.css": "^8.0.1",
    "normalizr": "^3.3.0",
    "npm-license-crawler": "git+https://github.com/mwittig/npm-license-crawler.git#master",
    "ps-list": "^6.0.0",
    "query-string": "^6.9.0",
    "snakecase-keys": "^3.1.0",
    "uniqid": "^5.0.3",
    "universal-analytics": "^0.4.20",
    "v-tooltip": "^2.0.2",
    "vue": "^2.5.16",
    "vue-electron": "^1.0.6",
    "vue-feather-icons": "^5.0.0",
    "vue-i18n": "^8.14.0",
    "vue-js-modal": "^1.3.31",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.1.3",
    "vue-timers": "^2.0.4",
    "vue2-animate": "^2.1.3",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.8.4",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ajv": "^6.10.2",
    "axios-mock-adapter": "^1.17.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^2.4.5",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^3.4.2",
    "del": "^5.1.0",
    "devtron": "^1.4.0",
    "dotenv-webpack": "^1.7.0",
    "electron": "^6.1.5",
    "electron-builder": "^21.2.0",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-notarize": "^0.2.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.4",
    "inject-loader": "^4.0.1",
    "jest": "^24.9.0",
    "karma-coverage": "^1.1.2",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "0.9.0",
    "mockdate": "^2.0.2",
    "multispinner": "^0.2.1",
    "node-abi": "^2.13.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.12.0",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.3.1",
    "spectron": "^9.0.0",
    "style-loader": "^0.21.0",
    "url-loader": "^3.0.0",
    "vue-html-loader": "^1.2.4",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "vuex-mock-store": "0.0.7",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "setupFiles": [
      "./__tests__/__setups__/config.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "testPathIgnorePatterns": [
      "/__tests__/__setups__/",
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/renderer/$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/renderer/store/plugins/**",
      "!src/main/**",
      "!coverage/**",
      "!**/node_modules/**",
      "!**/__tests__/__setups__/**"
    ],
    "clearMocks": true
  }
}
